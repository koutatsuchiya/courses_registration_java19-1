<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Semester" table="semester" schema="public" catalog="registration">
        <id name="id">
            <column name="id" sql-type="integer"/>
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="integer"/>
        </property>
        <property name="year">
            <column name="year" sql-type="integer"/>
        </property>
        <property name="dayStart">
            <column name="day_start" sql-type="date"/>
        </property>
        <property name="dayEnd">
            <column name="day_end" sql-type="date"/>
        </property>
        <property name="current">
            <column name="current" sql-type="boolean"/>
        </property>
    </class>
</hibernate-mapping>